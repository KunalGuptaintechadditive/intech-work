	// Read the STL file
	vtkNew<vtkSTLReader> reader;
	reader->SetFileName(stl_file.c_str());
	reader->Update();

	vtkPolyData* shape = reader->GetOutput();

	vtkSmartPointer<vtkPolyData> polyData = reader->GetOutput();
